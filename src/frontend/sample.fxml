<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane
  fx:controller="frontend.Controller"
  xmlns:fx="http://javafx.com/fxml"
  prefWidth="1280"
  prefHeight="720">

  <Tab
    text="Product Line">

    <Button
      fx:id="btnProductLine"
      onMouseClicked="#prodLineBtnClicked"
      text="Product Line Button"/>
  </Tab>

  <Tab
    text="Record Production">

    <Button
      fx:id="btnRecordProd"
      text="Record Production Button"
      onMouseClicked="#recordBtnClicked"/>

  </Tab>

  <Tab
    text="Production Lab">

    <Button
      fx:id="btnProdLab"
      text="Production Lab Button"
      onMouseClicked="#prodLabBtnClicked"/>

  </Tab>
</TabPane>